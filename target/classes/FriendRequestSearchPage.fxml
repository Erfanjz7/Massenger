<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="388.0" prefWidth="388.0" style="-fx-background-color: white; -fx-border-width: 10; -fx-border-color: green;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.FriendRequestSearchPage">
    <children>
        <Label layoutX="115.0" layoutY="14.0" text="Send Request" textFill="WHITE">
            <font>
                <Font name="Cooper Black" size="23.0" />
            </font>
        </Label>
        <TextField fx:id="usernameTXTFLD" layoutX="120.0" layoutY="162.0" promptText="Username :" />
        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="87.0" layoutY="83.0" prefHeight="67.0" prefWidth="215.0" text="Enter a username to search :" textAlignment="CENTER" textFill="GREEN">
            <font>
                <Font name="Trebuchet MS" size="16.0" />
            </font>
        </Label>
        <Button fx:id="findBTN" layoutX="152.0" layoutY="260.0" mnemonicParsing="false" onAction="#findBTNHandler" prefHeight="29.0" prefWidth="83.0" text="Find" textFill="GREEN">
            <font>
                <Font name="Trebuchet MS" size="16.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
        <Button fx:id="backBTN" layoutX="16.0" layoutY="327.0" mnemonicParsing="false" onAction="#backBTNHandler" prefHeight="26.0" prefWidth="107.0" text="Back" textFill="GREEN">
            <font>
                <Font name="Trebuchet MS" size="20.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
    </children>
</AnchorPane>
